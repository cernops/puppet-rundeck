RDldaplogin {
    com.dtolabs.rundeck.jetty.jaas.JettyCachingLdapLoginModule required
        debug="true"
        contextFactory="com.sun.jndi.ldap.LdapCtxFactory"
        providerUrl="ldap://<%= @ldap_config['server'] %>:<%= @ldap_config['port'] %>"
        authenticationMethod="simple"
        forceBindingLogin="<%= @ldap_config['force_binding'] %>"<% if @ldap_config['bind_dn'] %>
        bindDn="CN=<%= @ldap_config['bind_dn']%>,<%= @ldap_config['user_base_dn'] %>"<% end %><% if @ldap_config['bind_password'] %>
        bindPassword="<%= @ldap_config['bind_password']%>"<% end %>
        userBaseDn="<%= @ldap_config['user_base_dn'] %>"
        userRdnAttribute="<%= @ldap_config['user_rdn_attribute'] %>"
        userIdAttribute="<%= @ldap_config['user_id_attribute'] %>"
        userObjectClass="<%= @ldap_config['user_object_class'] %>"
        roleBaseDn="<%= @ldap_config['role_base_dn'] %>"
        roleNameAttribute="<%= @ldap_config['role_name_attribute'] %>"
        roleMemberAttribute="<%= @ldap_config['role_member_attribute'] %>"
        roleObjectClass="<%= @ldap_config['role_object_class'] %>"<% if @ldap_config['supplemental_roles'] %>
        supplementalRoles="<%= @ldap_config['supplemental_roles'] %>"<% end %>
        cacheDurationMillis="300000"
        reportStatistics="true"
        nestedGroups="<%= @ldap_config['nested_groups'] %>";
};
